{
  "source": {
    "include": ["src"],
    "includePattern": ".+(\\.js)$",
    "excludePattern": ".+\\.test\\.js$"
  },
  "opts": {
    "destination": "./docs/pages",
    "recurse": true,
    "template": "node_modules/minami",
    "readme": "./docs/README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "idInHeadings": true
  }
}
